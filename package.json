  "license": "MIT",
  "scripts": {
    "start": "nodemon server",
    "test": "mocha ./server/*/__test__/*.spec.js --recursive || true",
    "coverage": "clear && mocha ./server/*/__test__/*.spec.js --recursive && istanbul cover _mocha $(find ./server/*/ -name \"*.js\" -not -path \"./node_modules/*\" -not -name \"*.spec.js\" -not -name \"*.utils.js\") || true && open coverage/lcov-report/index.html "
  },
  "private": true,
  "dependencies": {
    "async": "2.4.1",
    "bcrypt": "1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.17.2",
    "express": "4.15.3",
    "express-validation": "^1.0.2",
    "express-validator": "3.2.0",
    "joi": "^11.0.2",
    "jsonwebtoken": "7.4.1",
    "mongoose": "4.10.5",
    "nodemailer": "2.7.2",
    "passport": "0.3.2",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "regex-collection": "file:local_modules/regex-collection"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "3.0.0",
    "mocha": "^3.5.3",
    "sinon": "^3.2.1",
    "sinon-express-mock": "^1.3.1",
    "sinon-mongoose": "^2.0.2"
  }
